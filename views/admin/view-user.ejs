<div class="main-panel">
    <div class="content-wrapper">
      <div class="row">
<div class="col-lg-12 grid-margin">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title"></h4>
        <p class="card-description">
         
        </p>
        <div class="table-responsive">
          <table class="table table-striped">
            <thead>
              <tr>
                <th>
                 Name
                </th>
                <th>
                 Email
                </th>
                <th>
                 Phone number
                </th>
                <th>
                  Active
                </th>
                
              </tr>
            </thead>
            <tbody>
                <% users.forEach(user => { %>
                    <tr>
                     
                      <td>
                        <%= user.name %> 
                      </td>
                      <td>
                          <%= user.email %> 
                      </td>
                      <td>
                          <%= user.phone   %> 
                      </td>
                      <td>
                          <% if(user.active===false){ %> 
                              <a href="unblock-user?id=<%= user._id  %> " onclick="return confirm('Want to unblock the user?');">
                                  <button type="button" class="btn btn-danger shadow">UnBlock</button>
                              </a>
                          <% }else{ %> 
                              <a href="block-user?id=<%= user._id  %> " onclick="return confirm('Want to Block the user?');">
                                  <button type="button" class="btn btn-outline-danger shadow">Block</button>
                              </a>
                          <% } %> 
                      </td>
                    </tr>
  <%  }); %> 



            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
      </div></div>